  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.37",
    "@tailwindcss/vite": "^4.1.8",
    "@types/aos": "^3.0.7",
    "@types/bun": "^1.2.15",
