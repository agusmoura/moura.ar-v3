{
  "name": "moura.ar-v3",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev",
    "dev external": "bunx --bun astro dev --host",
    "build": "bunx --bun astro build",
    "start": "node ./dist/server/entry.mjs",
    "preview": "bunx --bun astro preview",
    "build:test": "bunx --bun astro build && bun run start",
    "astro": "bunx --bun astro",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check": "astro check && tsc --noEmit",
    "analyze": "bunx --bun vite-bundle-visualizer",
    "test": "vitest",
    "test:run": "vitest run",
    "test:contact": "vitest run src/utils/__tests__/contact-form.test.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/node": "^9.2.2",
    "@astrojs/sitemap": "^3.4.1",
    "@fontsource-variable/noto-serif-jp": "^5.2.5",
    "@fontsource/space-mono": "^5.2.5",
    "@types/jsonwebtoken": "^9.0.10",
    "aos": "^2.3.4",
    "astro": "5.9.2",
    "jsonwebtoken": "^9.0.2",
    "rehype-raw": "^7.0.0",
    "swiper": "^11.2.8",
    "typescript": "^5.8.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.37",
    "@tailwindcss/vite": "^4.1.8",
    "@types/aos": "^3.0.7",
    "@types/bun": "^1.2.15",
    "@types/node": "^22.15.24",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitest/coverage-v8": "^3.2.4",
    "astro-icon": "^1.1.5",
    "eslint": "^9.27.0",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "lightningcss": "^1.30.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.8",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^3.2.4"
  }
}
